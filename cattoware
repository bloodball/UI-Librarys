-- we do a little trolling. go into source for rest of func
local library =
    loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/cat", true))()
local window = library:CreateWindow("name", UDim2.new(492, 598), Enum.KeyCode.RightShift) -- 2nd argument is the size, 3rd is the show/hide ofc
local tab = window:CreateTab("Tab")
local sector = tab:CreateSector("Sector", "left") -- can be "right"
local button =
    sector:AddButton(
    "Button",
    function()
        print("button")
    end
)
local label = sector:AddLabel("Label")
